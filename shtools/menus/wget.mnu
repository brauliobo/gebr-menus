    <url>http://www.gnu.org/software/wget/</url>
    <parameters default-selection="0">
      <parameter>
        <label>Continue getting a partially-downloaded file</label>
        <flag>
          <property required="no">
            <keyword>--continue</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Select the type of the progress indicator you wish to use</label>
        <enum>
          <property required="no">
            <keyword>--progress=</keyword>
            <value/>
            <default>dot</default>
          </property>
          <option>
            <label>Dot</label>
            <value>dot</value>
          </option>
          <option>
            <label>Bar</label>
            <value>bar</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Turn off output</label>
        <flag>
          <property required="no">
            <keyword>--quiet</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Turn on recursive retrieving</label>
        <flag>
          <property required="no">
            <keyword>--recursive</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Do not create a hierarchy of directories when retrieving recursively</label>
        <flag>
          <property required="no">
            <keyword>--no-directories</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Specify comma-separated lists of suffixes to accept</label>
        <string>
          <property required="no" separator=",">
            <keyword>--accept</keyword>
            <value/>
            <default/>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>Ignore case when matching files and directories</label>
        <flag>
          <property required="no">
            <keyword>--ignore-case</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>The URL that contains the file(s) to be downloaded</label>
        <string>
          <property required="yes">
            <keyword/>
            <value/>
            <default/>
          </property>
        </string>
      </parameter>
    </parameters>
  </program>
