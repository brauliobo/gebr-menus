    <url>http://chicago.sourceforge.net/xlhtml/</url>
    <parameters default-selection="0" selection="">
      <parameter>
        <label>Centers tables horizontally</label>
        <flag>
          <property required="no">
            <keyword>-c</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Ignore colors in output</label>
        <flag>
          <property required="no">
            <keyword>-nc</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Suppress header and body tags in HTML</label>
        <flag>
          <property required="no">
            <keyword>-nh</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Trims empty rows and columns at the edges of a worksheet</label>
        <flag>
          <property required="no">
            <keyword>-te</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Suppress formula warnings about accuracy</label>
        <flag>
          <property required="no">
            <keyword>-fw</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>XML output</label>
        <flag>
          <property required="no">
            <keyword>-xml</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>MS Excel file</label>
        <file directory="no" filter-name="MS Excel file" filter-pattern="*.xls *.XLS">
          <property required="yes">
            <keyword/>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
    </parameters>
  </program>
