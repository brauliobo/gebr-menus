    <url>http://gcc.gnu.org/</url>
    <parameters default-selection="0">
      <parameter>
        <label>Source file(s)</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="yes" separator=" ">
            <keyword />
            <value></value>
            <default />
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Compilation type</label>
        <enum>
          <property required="no">
            <keyword />
            <value />
            <default />
          </property>
          <option>
            <label>Preprocess only; do not compile, assemble or link</label>
            <value>-E</value>
          </option>
          <option>
            <label>Compile only; do not assemble or link</label>
            <value>-S</value>
          </option>
          <option>
            <label>Compile and assemble, but do not link</label>
            <value>-c</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Output file</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>-o</keyword>
            <value></value>
            <default />
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Ansi</label>
        <flag>
          <property required="no">
            <keyword>-ansi</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Pedantic</label>
        <flag>
          <property required="no">
            <keyword>-pedantic</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Include and Lib directories</label>
        <group instanciable="yes" expand="yes">
          <template-instance>
            <parameters default-selection="0">
              <parameter>
                <label>Include directory</label>
                <file directory="yes" filter-name="" filter-pattern="">
                  <property required="no">
                    <keyword>-I</keyword>
                    <value />
                    <default />
                  </property>
                </file>
              </parameter>
              <parameter>
                <label>Lib directory</label>
                <file directory="yes" filter-name="" filter-pattern="">
                  <property required="no">
                    <keyword>-L</keyword>
                    <value />
                    <default />
                  </property>
                </file>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0">
            <parameter>
              <label>Include directory</label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default />
                </property>
              </reference>
            </parameter>
            <parameter>
              <label>Lib directory</label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default />
                </property>
              </reference>
            </parameter>
          </parameters>
        </group>
      </parameter>
      <parameter>
        <label>Put here other options you want</label>
        <group instanciable="yes" expand="no">
          <template-instance>
            <parameters default-selection="0">
              <parameter>
                <label>Option</label>
                <string>
                  <property required="no">
                    <keyword />
                    <value />
                    <default />
                  </property>
                </string>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0">
            <parameter>
              <label>Put here the other options you want</label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default />
                </property>
              </reference>
            </parameter>
          </parameters>
        </group>
      </parameter>
    </parameters>
  </program>
