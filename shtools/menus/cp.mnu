    <parameters default-selection="0">
      <parameter>
        <label>Copy directories recursively</label>
        <flag>
          <property required="no">
            <keyword>-R</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Only update files</label>
        <flag>
          <property required="no">
            <keyword>-u</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Always follow symbolic links in SOURCE</label>
        <flag>
          <property required="no">
            <keyword>-L</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Do not overwrite an existing file</label>
        <flag>
          <property required="no">
            <keyword>-n</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Make symbolic links instead of copying</label>
        <flag>
          <property required="no">
            <keyword>-s</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Explain what is being done</label>
        <flag>
          <property required="no">
            <keyword>-v</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      
    <parameter><label>Put here other options you want</label><group instanciable="yes" expand="no"><template-instance><parameters default-selection="0"><parameter><label>Option</label><string><property required="no"><keyword/><value/><default/></property></string></parameter></parameters></template-instance><parameters default-selection="0"><parameter><label>Put here the other options you want</label><reference><property required="no"><keyword/><value/><default/></property></reference></parameter></parameters></group></parameter><parameter><label>Source </label><group instanciable="yes" expand="yes"><template-instance><parameters default-selection="1" selection="1"><parameter><label>File</label><file directory="no" filter-name="" filter-pattern=""><property required="yes"><keyword/><value/><default/></property></file></parameter><parameter><label>Directory</label><file directory="yes" filter-name="" filter-pattern=""><property required="yes"><keyword/><value/><default/></property></file></parameter></parameters></template-instance><parameters default-selection="1" selection="1"><parameter><label/><reference><property required="no"><keyword/><value/><default/></property></reference></parameter><parameter><label/><reference><property required="no"><keyword/><value/><default/></property></reference></parameter></parameters></group></parameter><parameter><label>Destiny directory</label><file directory="yes" filter-name="" filter-pattern=""><property required="yes"><keyword/><value/><default/></property></file></parameter></parameters>
