    <url>http://www.gnu.org/software/coreutils/</url>
    <parameters default-selection="0" selection="">
      <parameter>
        <label>Show complete information</label>
        <flag>
          <property required="no">
            <keyword>-a</keyword>
            <value>off</value>
            <default>on</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Print the network node hostname</label>
        <flag>
          <property required="no">
            <keyword>-n</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Kernel name</label>
        <flag>
          <property required="no">
            <keyword>-s</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Kernel release</label>
        <flag>
          <property required="no">
            <keyword>-n</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Kernel version</label>
        <flag>
          <property required="no">
            <keyword>-v</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Machine hardware name</label>
        <flag>
          <property required="no">
            <keyword>-m</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Processor type</label>
        <flag>
          <property required="no">
            <keyword>-p</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Hardware platform</label>
        <flag>
          <property required="no">
            <keyword>-i</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Operating system</label>
        <flag>
          <property required="no">
            <keyword>-o</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
