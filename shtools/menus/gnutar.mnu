    <parameters default-selection="0" selection="">
      <parameter>
        <label>Behaviour</label>
        <enum>
          <property required="yes">
            <keyword />
            <value />
            <default />
          </property>
          <option>
            <label>create a new archive</label>
            <value>-c</value>
          </option>
          <option>
            <label>extract files from an archive</label>
            <value>-x</value>
          </option>
          <option>
            <label>append tar files to an archive</label>
            <value>-A</value>
          </option>
          <option>
            <label>delete from the archive</label>
            <value>--delete</value>
          </option>
          <option>
            <label>append files to the end of an archive</label>
            <value>-r</value>
          </option>
          <option>
            <label>list the contents of an archive</label>
            <value>-t</value>
          </option>
          <option>
            <label>only append files that are newer than copy in archive</label>
            <value>-u</value>
          </option>
          <option>
            <label>compare archive and file system</label>
            <value>--compare</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Change to directory</label>
        <file directory="yes">
          <property required="no">
            <keyword>-C</keyword>
            <value></value>
            <default />
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Compression type</label>
        <enum>
          <property required="no">
            <keyword />
            <value />
            <default />
          </property>
          <option>
            <label>bzip2</label>
            <value>--bzip2</value>
          </option>
          <option>
            <label>gzip</label>
            <value>--gzip</value>
          </option>
          <option>
            <label>compress</label>
            <value>-Z</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>File selection options</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0" selection="">
              <parameter>
                <label>Stay in local file system when creating an archive</label>
                <flag>
                  <property required="no">
                    <keyword>--one-file-system</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Do not recurse into subdirectories</label>
                <flag>
                  <property required="no">
                    <keyword>--no-recursion</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>File with a list of files to extract or archive</label>
                <file directory="no">
                  <property required="no">
                    <keyword>-T</keyword>
                    <value />
                    <default />
                  </property>
                </file>
              </parameter>
              <parameter>
                <label>Exclude files that match this pattern</label>
                <string>
                  <property required="no">
                    <keyword>--exclude=</keyword>
                    <value />
                    <default />
                  </property>
                </string>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0" selection="">
            <parameter>
              <label />
              <reference>
                <property required="no">
                  <keyword />
                  <value>off</value>
                  <default>off</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label />
              <reference>
                <property required="no">
                  <keyword />
                  <value>off</value>
                  <default>off</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label />
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default />
                </property>
              </reference>
            </parameter>
            <parameter>
              <label />
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default />
                </property>
              </reference>
            </parameter>
          </parameters>
        </group>
      </parameter>
      <parameter>
        <label>Setup options</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0" selection="">
              <parameter>
                <label>List files processed</label>
                <flag>
                  <property required="no">
                    <keyword>-v</keyword>
                    <value>off</value>
                    <default>on</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Don’t dump symlinks, but archive and dump the files they point to</label>
                <flag>
                  <property required="no">
                    <keyword>--dereference</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Display checkpoints at every given interval</label>
                <int>
                  <property required="no">
                    <keyword>--checkpoint=</keyword>
                    <value />
                    <default>10</default>
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>Ignore case when excluding files</label>
                <flag>
                  <property required="no">
                    <keyword>--ignore-case</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Don't replace existing files when extracting</label>
                <flag>
                  <property required="no">
                    <keyword>--keep-old-files</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Extract files to standard output instead to disk</label>
                <flag>
                  <property required="no">
                    <keyword>--to-stdout</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Don't strip leading '/' from file names (absolute path)</label>
                <flag>
                  <property required="no">
                    <keyword>--absolute-names</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Backup before removal and append specified suffix</label>
                <string>
                  <property required="no">
                    <keyword>--suffix=</keyword>
                    <value />
                    <default />
                  </property>
                </string>
              </parameter>
              <parameter>
                <label>Remove files after adding them to the archive</label>
                <flag>
                  <property required="no">
                    <keyword>--remove-files</keyword>
                    <value>off</value>
                    <default>off</default>
                  </property>
                </flag>
              </parameter>
              <parameter>
                <label>Tape length (multiples of 1024 bytes)</label>
                <int>
                  <property required="no">
                    <keyword>-L</keyword>
                    <value />
                    <default />
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>While adding files, set owner to</label>
                <string>
                  <property required="no">
                    <keyword>--owner=</keyword>
                    <value />
                    <default />
                  </property>
                </string>
              </parameter>
              <parameter>
                <label>While adding files, set group to</label>
                <string>
                  <property required="no">
                    <keyword>--group=</keyword>
                    <value />
                    <default />
                  </property>
                </string>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0" selection="">
            <parameter>
              <label />
              <reference>
                <property required="no">
                  <keyword />
                  <value>off</value>
                  <default>on</default>
