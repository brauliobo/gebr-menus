          <parameter><label>Automatic CMP stack</label><flag><property required="no"><keyword>autoCMPstack=1 ZOsearch=0 Inistack=0 Optimize=0 Supergathers=0 migmap=0</keyword><value>off</value><default>on</default></property></flag></parameter><parameter><label>Zero-offset search/stack</label><flag><property required="no"><keyword>autoCMPstack=0 ZOsearch=1 Inistack=0 Optimize=0 Supergathers=0 migmap=0</keyword><value>off</value><default>on</default></property></flag></parameter><parameter><label>Initial CRS stack</label><flag><property required="no"><keyword>autoCMPstack=0 ZOsearch=0 Inistack=1 Optimize=0 Supergathers=0 migmap=0</keyword><value>off</value><default>on</default></property></flag></parameter><parameter><label>Attribute optimization and stack</label><flag><property required="no"><keyword>autoCMPstack=0 ZOsearch=0 Inistack=0 Optimize=1 Supergathers=0 migmap=0</keyword><value>off</value><default>on</default></property></flag></parameter><parameter><label>Generate CRS supergathers</label><flag><property required="no"><keyword>autoCMPstack=0 ZOsearch=0 Inistack=0 Optimize=0 Supergathers=1 migmap=0</keyword><value>off</value><default>on</default></property></flag></parameter><parameter><label>Generate migration map</label><flag><property required="no"><keyword>autoCMPstack=0 ZOsearch=0 Inistack=0 Optimize=0 Supergathers=0 migmap=1</keyword><value>off</value><default>on</default></property></flag></parameter></parameters></template-instance>
        <parameters default-selection="1" selection="1">
            
            
            
            
            
          <parameter><label/><reference><property required="no"><keyword/><value/><default>off</default></property></reference></parameter><parameter><label/><reference><property required="no"><keyword/><value/><default>off</default></property></reference></parameter><parameter><label/><reference><property required="no"><keyword/><value/><default>off</default></property></reference></parameter><parameter><label/><reference><property required="no"><keyword/><value/><default>off</default></property></reference></parameter><parameter><label/><reference><property required="no"><keyword/><value/><default>off</default></property></reference></parameter><parameter><label/><reference><property required="no"><keyword/><value/><default>off</default></property></reference></parameter></parameters></group>
      </parameter><parameter><label>General Parameters</label><group instanciable="no" expand="yes"><template-instance><parameters default-selection="0"><parameter>
              <label>Pre-stack data file</label>
              <file directory="no" filter-name="SU" filter-pattern="*.su *.SU">
                <property required="yes">
                  <keyword>input=</keyword>
                  <value/>
                  <default/>
                </property>
              </file>
            </parameter><parameter>
              <label>Pathname for I/O data files</label>
