      <parameter>
        <label>Target zone parameters</label>
        <group instanciable="no" expand="no">
          <template-instance><parameters default-selection="0" selection="">
            <parameter>
              <label>Smallest CDP to simulated</label>
              <int>
                <property required="no">
                  <keyword>fcpd=</keyword>
