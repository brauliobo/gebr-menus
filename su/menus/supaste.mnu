    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of samples per trace</label>
        <int>
          <property required="yes">
            <keyword>ns=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>File with SEG-Y headers</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>head=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Format to write binary file</label>
        <enum>
          <property required="no">
            <keyword>ftn=</keyword>
            <value/>
            <default>0</default>
          </property>
          <option>
            <label>Unformated C</label>
            <value>0</value>
          </option>
          <option>
            <label>Unformated Fortran</label>
            <value>1</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
