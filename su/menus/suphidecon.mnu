    <parameters default-selection="0">
      <parameter><label>Time range used for wavelet extraction</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter>
        <label>Pre zero time</label>
        <float>
          <property required="no">
            <keyword>tm=</keyword>
            <value/>
            <default>-0.1</default>
          </property>
        </float>
      </parameter><parameter>
        <label>Post zero time</label>
        <float>
          <property required="no">
            <keyword>tp=</keyword>
            <value/>
            <default>0.4</default>
          </property>
        </float>
      </parameter></parameters></template-instance><parameters default-selection="0"><parameter>
        <label>Post zero time</label>
        
      <reference><property required="no"><keyword/><value/><default>-0.1</default></property></reference></parameter><parameter>
        <label>Post zero time (minimum phase component + multiples)</label>
        
      <reference><property required="no"><keyword/><value/><default>0.4</default></property></reference></parameter></parameters></group></parameter>
      
      
      
    <parameter>
        <label>Pre-withening (assumed noise to prevent division by zero)</label>
        <float>
          <property required="no">
            <keyword>pnoise=</keyword>
            <value/>
            <default>0.001</default>
          </property>
        </float>
      </parameter></parameters>
  </program>
