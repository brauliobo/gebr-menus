    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of samples in 1st (fast) dimension</label>
        <int>
          <property required="no">
            <keyword>n1=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of samples in 2nd (slow) dimension</label>
        <int>
          <property required="no">
            <keyword>n2=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Sampling interval in 1st dimension</label>
        <float>
          <property required="no">
            <keyword>d1=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>First sample in 1st dimension</label>
        <float>
          <property required="no">
            <keyword>f1=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Parameters of 1st dimension after resampling</label>
        <group instanciable="no" expand="no">
          <template-instance><parameters default-selection="0" selection="">
            <parameter>
              <label>Number of samples</label>
              <int>
                <property required="no">
                  <keyword>n1r=</keyword>
                  <value/>
                  <default/>
                </property>
              </int>
            </parameter>
            <parameter>
              <label>Sampling interval</label>
              <float>
                <property required="no">
                  <keyword>d1r=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>First sample</label>
              <float>
                <property required="no">
                  <keyword>f1r=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
          </parameters></template-instance>
        <parameters default-selection="0" selection="">
            <parameter>
              <label>First sample</label>
              <int>
                <property required="no">
                  <keyword>n1r=</keyword>
                  <value/>
                  <default/>
                </property>
              </int>
            </parameter>
            <parameter>
              <label>Sampling interval in 1st dimension after resampling</label>
              <float>
                <property required="no">
                  <keyword>d1r=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>First sample in 1st dimension after resampling</label>
              <float>
                <property required="no">
                  <keyword>f1r=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
          </parameters></group>
      </parameter>
    </parameters>
