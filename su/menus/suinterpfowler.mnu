    <parameters default-selection="0">
      <parameter><label>Velocity panel properties</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter>
        <label>Minimum velocity</label>
        <float>
          <property required="no">
            <keyword>vmin=</keyword>
            <value/>
            <default>1500.0</default>
          </property>
        </float>
      </parameter><parameter>
        <label>Maximum velocity</label>
        <float>
          <property required="no">
            <keyword>vmax=</keyword>
            <value/>
            <default>2500.0</default>
          </property>
        </float>
      </parameter><parameter>
        <label>Number of velocity panels</label>
        <int>
          <property required="no">
            <keyword>nv=</keyword>
            <value/>
            <default>21</default>
          </property>
        </int>
      </parameter></parameters></template-instance><parameters default-selection="0"><parameter>
        <label>Minimum velocity</label>
        
      <reference><property required="no"><keyword/><value/><default>1500.0</default></property></reference></parameter><parameter>
        <label>Maximum velocity</label>
        
      <reference><property required="no"><keyword/><value/><default>2500.0</default></property></reference></parameter><parameter>
        <label>Number of velocity panels</label>
        
      <reference><property required="no"><keyword/><value/><default>21</default></property></reference></parameter></parameters></group></parameter><parameter><label>ETA values properties</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter>
        <label>Minimum ETA value</label>
        <float>
          <property required="no">
            <keyword>etamin=</keyword>
            <value/>
            <default>0.10</default>
          </property>
        </float>
      </parameter><parameter>
        <label>Maximum ETA value</label>
        <float>
          <property required="no">
            <keyword>etamax=</keyword>
            <value/>
            <default>0.25</default>
          </property>
        </float>
      </parameter><parameter>
        <label>Number of ETA values to image</label>
        <int>
          <property required="no">
            <keyword>neta=</keyword>
            <value/>
            <default>11</default>
          </property>
        </int>
      </parameter></parameters></template-instance><parameters default-selection="0"><parameter>
        <label>Minimum ETA value</label>
        
      <reference><property required="no"><keyword/><value/><default>0.10</default></property></reference></parameter><parameter>
        <label>Maximum ETA value</label>
        
      <reference><property required="no"><keyword/><value/><default>0.25</default></property></reference></parameter><parameter>
        <label>Number of ETA values to image</label>
        
      <reference><property required="no"><keyword/><value/><default>11</default></property></reference></parameter></parameters></group></parameter><parameter>
        <label>Number of CDP points</label>
        <int>
          <property required="no">
            <keyword>ncdps=</keyword>
            <value/>
            <default>1130</default>
          </property>
        </int>
      </parameter>
      
      
      
      
      
      
      
      <parameter><label>Input data from CVS</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter>
        <label>Minimum velocity specified for CVS</label>
        <float>
          <property required="no">
            <keyword>vminstack=</keyword>
            <value/>
            <default>1450</default>
          </property>
        </float>
      </parameter><parameter>
        <label>Number of constant velocity stack panels output by CVS</label>
        <int>
          <property required="no">
            <keyword>nvstack=</keyword>
            <value/>
            <default>0</default>
          </property>
        </int>
      </parameter><parameter><label>Scale factor for velocity functions</label><float><property required="no"><keyword>vscale=</keyword><value/><default>1.0</default></property></float></parameter></parameters></template-instance><parameters default-selection="0"><parameter>
        <label>Minimum velocity specified for CVS</label>
        
      <reference><property required="no"><keyword/><value/><default>1450</default></property></reference></parameter><parameter>
        <label>Number of constant velocity stack panels output by CVS</label>
        
      <reference><property required="no"><keyword/><value/><default>0</default></property></reference></parameter><parameter><label>Scale factor for velocity functions</label><reference><property required="no"><keyword/><value/><default>1.0</default></property></reference></parameter></parameters></group></parameter>
    <parameter><label>Parameter to specify the desired output</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter><label>CDP number for (t,v,eta) triplets</label><int><property required="no" separator=","><keyword>cdp=</keyword><value/><default>0</default></property></int></parameter><parameter><label>Array of times for (t,v,eta) triplets</label><float><property required="no" separator=","><keyword>t=</keyword><value/><default>0.0</default></property></float></parameter><parameter><label>Array of velocities for (t,v,eta) triplets</label><float><property required="no" separator=","><keyword>v=</keyword><value/><default>1500.0</default></property></float></parameter><parameter><label>Array of ETAs for (t,v,eta) triplets</label><float><property required="no" separator=","><keyword>eta=</keyword><value/><default>0.0</default></property></float></parameter></parameters></template-instance><parameters default-selection="0"><parameter><label>CDP number for (t,v,eta) triplets</label><reference><property required="no"><keyword/><value/><default/></property></reference></parameter><parameter><label>Array of times for (t,v,eta) triplets</label><reference><property required="no"><keyword/><value/><default>0.0</default></property></reference></parameter><parameter><label>Array of velocities for (t,v,eta) triplets</label><reference><property required="no"><keyword/><value/><default>1500.0</default></property></reference></parameter><parameter><label>Array of ETAs for (t,v,eta) triplets</label><reference><property required="no"><keyword/><value/><default>0.0</default></property></reference></parameter></parameters></group></parameter></parameters>
