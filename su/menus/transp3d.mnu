    <parameters default-selection="0">
      <parameter>
        <label>Number of elements in 1st (fast) dimension of matrix</label>
        <int>
          <property required="yes">
            <keyword>n1=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of elements in 2nd (middle) dimension of matrix</label>
        <int>
          <property required="yes">
            <keyword>n2=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of elements in 3rd (slow) dimension of matrix</label>
        <int>
          <property required="no">
            <keyword>n3=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Desired output axis ordering</label>
        <int>
          <property required="no">
            <keyword>perm=</keyword>
            <value/>
            <default>231</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of bytes per matrix element</label>
        <int>
          <property required="no">
            <keyword>nbpe=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Directory for scratch files</label>
        <file directory="yes" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>scratchdir=</keyword>
            <value/>
            <default>/tmp</default>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Stem prefix for scratch file names</label>
        <string>
          <property required="no">
            <keyword>scratchstem=</keyword>
            <value/>
            <default>foo</default>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
