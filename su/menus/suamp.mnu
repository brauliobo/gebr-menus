      <parameter><label>Output mode</label><enum><property required="yes"><keyword>mode=</keyword><value/><default>amp</default></property><option><label>Amplitude traces</label><value>amp</value></option><option><label>Phase traces</label><value>phase</value></option><option><label>Real parts</label><value>real</value></option><option><label>Imaginary parts</label><value>imag</value></option></enum></parameter>
    <parameter><label>Divide value at zero frequency by 2</label><flag><property required="no"><keyword>jack=1</keyword><value>off</value><default>off</default></property></flag></parameter></parameters>
