    <url>http://www.cwp.mines.edu/cwpcodes/</url><parameters default-selection="0" selection="">
      <parameter><label>Alpha (vertical p velocity)</label><float><property required="no"><keyword>a=</keyword><value/><default>2500</default></property>
        
        
        
      </float></parameter>
      <parameter><label>Beta (vertical sv velocity)</label><float><property required="no"><keyword>b=</keyword><value/><default>1250</default></property>
        
        
        
      </float></parameter>
      <parameter><label>Epsilon (horiz p-wave anisotropy)</label><float><property required="no"><keyword>e=</keyword><value/><default>0.20</default></property>
        
        
        
      </float></parameter>
      <parameter><label>Delta (strange parameter)</label><float><property required="no"><keyword>d=</keyword><value/><default>0.10</default></property>
        
        
        
      </float></parameter>
      <parameter><label>Number of angles to compute</label><int><property required="no"><keyword>nangle=</keyword><value/><default>9001</default></property>
        
        
        
      </int></parameter>
      <parameter><label>Maximum angle in degrees</label><float><property required="no"><keyword>maxangle=</keyword><value/><default>90.0</default></property>
        
        
        
      </float></parameter>
      
      <parameter><label>Number of slowness</label><int><property required="no"><keyword>np=</keyword><value/><default>8001</default></property>
        
        
        
      </int></parameter>
      <parameter><label>Ouput</label><enum><property required="no"><keyword>option=</keyword><value/><default/></property><option><label>Velocity NMO (p)</label><value>1</value></option><option><label>Velocity NMO (theta) in degrees</label><value>2</value></option><option><label>Velocity NMO (theta) in radians</label><value>3</value></option><option><label>Velocity of phase (p)</label><value>4</value></option><option><label>velocity of phase (theta) in degrees</label><value>5</value></option><option><label>Velocity of phase (theta) in radians</label><value>6</value></option><option><label>Fist derivative of velocity of phase (p)</label><value>7</value></option><option><label>Fist derivative of velocity of phase (theta) in degrees</label><value>8</value></option><option><label>Fist derivative of velocity of phase (theta) in radians</label><value>9</value></option><option><label>Second derivative of velocity of phase (p)</label><value>10</value></option><option><label>Second derivative of velocity of phase (theta) in degrees</label><value>11</value></option><option><label>Second derivative of velocity of phase (theta) in radians</label><value>12</value></option><option><label>(1/Velocity NMO(0)^2-1/Velocity NMO(theta)^2)/p^2 test vs theta</label><value>13</value></option><option><label>Velocity NMO for weak anisotropy</label><value>14</value></option></enum></parameter>
      <parameter><label>Normalize</label><flag><property required="no"><keyword>normalize=1</keyword><value>off</value><default>off</default></property>
        
        
        
      </flag></parameter>
    <parameter><label>Echoes extra information</label><flag><property required="no"><keyword>verbose=1</keyword><value>off</value><default>off</default></property>
        
        
        
      </flag></parameter></parameters>
  </program>
</flow>
