      <parameter>
        <label>File containing new 3200 byte text header</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="yes">
            <keyword>text=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>File containing SEG-Y data set</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="yes">
            <keyword>data=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
    </parameters>
