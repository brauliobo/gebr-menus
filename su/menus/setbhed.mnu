                <property required="no">
                  <keyword/>
                  <value/>
                  <default/>
                </property>
              </reference>
            </parameter>
          </parameters>
        </group>
      </parameter>
      <parameter>
        <label>Correlated data traces code</label>
        <enum>
          <property required="no">
            <keyword>hcorr=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>No</label>
            <value>1</value>
          </option>
          <option>
            <label>Yes</label>
            <value>2</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Binary gain recovered code</label>
        <enum>
          <property required="no">
            <keyword>bgrcv=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>Yes</label>
            <value>1</value>
          </option>
          <option>
            <label>No</label>
            <value>2</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Amplitude recovery method code</label>
        <enum>
          <property required="no">
            <keyword>rcvm=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>None</label>
            <value>1</value>
          </option>
          <option>
            <label>Spherical divergence</label>
            <value>2</value>
          </option>
          <option>
            <label>AGC</label>
            <value>3</value>
          </option>
          <option>
            <label>Other</label>
            <value>4</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Measurement system code</label>
        <enum>
          <property required="no">
            <keyword>mfeet</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>Meters</label>
            <value>1</value>
          </option>
          <option>
            <label>Feet</label>
            <value>2</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Impulse signal polarity code</label>
        <enum>
          <property required="no">
            <keyword>polyt</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>Increase in pressure or upward geophone case movement gives negative number on tape</label>
            <value>1</value>
          </option>
          <option>
            <label>Increase in pressure or upward geophone case movement gives positive number on tape</label>
            <value>2</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Vibratory polarity code</label>
        <enum>
          <property required="no">
            <keyword>vpol=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>337.5 to 22.5 degrees</label>
            <value>1</value>
          </option>
          <option>
            <label>22.5 to 67.5 degrees</label>
            <value>2</value>
          </option>
          <option>
            <label>67.5 to 112.5 degrees</label>
            <value>3</value>
          </option>
          <option>
            <label>112.5 degrees to 157.5 degrees</label>
            <value>4</value>
          </option>
          <option>
            <label>157.5 to 202.5 degrees</label>
            <value>5</value>
          </option>
          <option>
            <label>202.5 to 247.5 degrees</label>
            <value>6</value>
          </option>
          <option>
            <label>247.5 to 292.5 degrees</label>
            <value>7</value>
          </option>
          <option>
            <label>293.5 to 337.5 degrees</label>
            <value>8</value>
          </option>
        </enum>
      </parameter>
