    <parameters default-selection="0" selection="">
      <parameter>
        <label>Output binary tape header file</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>bfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Parameter file</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>par=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Job identification number</label>
        <int>
          <property required="no">
            <keyword>jobid=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Line number (only one line per reel)</label>
        <int>
          <property required="no">
            <keyword>lino=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Reel number</label>
        <int>
          <property required="no">
            <keyword>reno=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of data traces per record</label>
        <int>
          <property required="no">
            <keyword>ntrpr=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of auxiliary traces per record</label>
        <int>
          <property required="no">
            <keyword>nart=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Sample interval in microsecs for this reel</label>
        <int>
          <property required="no">
            <keyword>hdt=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of samples per trace for this reel</label>
        <int>
          <property required="no">
            <keyword>hns=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Sample interval in microsecs for original field recording</label>
        <int>
          <property required="no">
            <keyword>dto=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of samples per trace for original field recording</label>
        <int>
          <property required="no">
            <keyword>nso=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>File format</label>
        <enum>
          <property required="no">
            <keyword>format=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>IBM floating point, 4 bytes</label>
            <value>1</value>
          </option>
          <option>
            <label>Two's complement integer, 4 bytes</label>
            <value>2</value>
          </option>
          <option>
            <label>Two's complement integer, 2 bytes</label>
            <value>3</value>
          </option>
          <option>
            <label>IEEE floating point, 4 bytes</label>
            <value>5</value>
          </option>
          <option>
            <label>Two's complement integer, 1 bytes</label>
            <value>8</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>CDP fold expected per CDP ensemble</label>
        <int>
          <property required="no">
            <keyword>fold=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Trace sorting code</label>
        <enum>
          <property required="no">
            <keyword>tsort=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>As recorded (no sorting)</label>
            <value>1</value>
          </option>
          <option>
            <label>CDP ensemble</label>
            <value>2</value>
          </option>
          <option>
            <label>Single fold continuous profile</label>
            <value>3</value>
          </option>
          <option>
            <label>Horizontally stacked</label>
            <value>4</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Vertical sum code</label>
        <enum>
          <property required="no">
            <keyword>vscode=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>No sum</label>
            <value>1</value>
          </option>
          <option>
            <label>Two sum</label>
            <value>2</value>
          </option>
          <option>
            <label>N sum (N = 32,767)</label>
            <value>N</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Sweep properties</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0" selection="">
              <parameter>
                <label>Sweep lenght</label>
                <int>
                  <property required="no">
                    <keyword>hslen=</keyword>
                    <value/>
                    <default/>
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>Sweep type code</label>
                <enum>
                  <property required="no">
                    <keyword>hstyp=</keyword>
                    <value/>
                    <default/>
                  </property>
                  <option>
                    <label>Linear</label>
                    <value>1</value>
                  </option>
                  <option>
                    <label>Parabolic</label>
                    <value>2</value>
                  </option>
                  <option>
                    <label>Exponential</label>
                    <value>3</value>
                  </option>
                  <option>
                    <label>Other</label>
                    <value>4</value>
                  </option>
                </enum>
              </parameter>
              <parameter>
                <label>Sweep frequency at end</label>
                <int>
                  <property required="no">
                    <keyword>hsfe=</keyword>
                    <value/>
                    <default/>
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>Sweep frequency at start</label>
                <int>
                  <property required="no">
                    <keyword>hsfs=</keyword>
                    <value/>
                    <default/>
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>Trace number of sweep channel</label>
                <int>
                  <property required="no">
                    <keyword>schn=</keyword>
                    <value/>
                    <default/>
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>Sweep trace taper length at start if tapered</label>
                <int>
                  <property required="no">
                    <keyword>hstas=</keyword>
                    <value/>
                    <default/>
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>Sweep trace taper lenght at end</label>
                <int>
                  <property required="no">
                    <keyword>hstae=</keyword>
                    <value/>
                    <default/>
                  </property>
                </int>
              </parameter>
              <parameter>
                <label>Sweep trace taper type code</label>
                <enum>
                  <property required="no">
                    <keyword>htatyp=</keyword>
                    <value/>
                    <default/>
                  </property>
                  <option>
                    <label>Linear</label>
                    <value>1</value>
                  </option>
                  <option>
                    <label>Cos-squared</label>
                    <value>2</value>
                  </option>
                  <option>
                    <label>Other</label>
                    <value>3</value>
                  </option>
                </enum>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0" selection="">
            <parameter>
              <label/>
              <reference>
                <property required="no">
                  <keyword/>
                  <value/>
