      <parameter><label>Gain whole data set (vs. trace by trace)</label><flag><property required="no"><keyword>panel=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter>
      <parameter><label>Multiply data by t^tpow</label><float><property required="no"><keyword>tpow=</keyword><value/><default>0.0</default></property>
	
	
	
      </float></parameter>
      <parameter><label>Multiply data by exp(epow*t)</label><float><property required="no"><keyword>epow=</keyword><value/><default/></property>
	
	
	
      </float></parameter>
      <parameter><label>Take signed gpowth power of scaled data</label><float><property required="no"><keyword>gpow=</keyword><value/><default/></property></float></parameter><parameter><label>Do automatic gain control</label><flag><property required="no"><keyword>agc=1</keyword><value>off</value><default>on</default></property>
	
	
	
      </flag></parameter>
      <parameter><label>With gaussian taper</label><flag><property required="no"><keyword>gagc=1</keyword><value>off</value><default>on</default></property>
	
	
	
      </flag></parameter>
      <parameter><label>AGC window in seconds (use if agc or gagc is checked)</label><float><property required="no"><keyword>wagc=</keyword><value/><default>0.5</default></property>
	
	
	
      </float></parameter>
      <parameter><label>Data clipping</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0" selection=""><parameter><label>Zero any value whose magnitude exceeds trapval</label><float><property required="no"><keyword>trap=</keyword><value/><default/></property>
	
	
	
      </float></parameter><parameter><label>Zero any value whose magnitude exceeds clipval</label><float><property required="no"><keyword>clip=</keyword><value/><default/></property>
	
	
	
      </float></parameter><parameter><label>Clip any value greater than</label><float><property required="no"><keyword>pclip=</keyword><value/><default/></property></float></parameter><parameter><label>Clip any value less than</label><float><property required="no"><keyword>nclip=</keyword><value/><default/></property></float></parameter><parameter><label>Clip by quantile on absolute values on trace</label><float><property required="no"><keyword>qclip=</keyword><value/><default/></property>
	
	
	
      </float></parameter></parameters></template-instance><parameters default-selection="0" selection=""><parameter><label>Zero any value whose magnitude exceeds trapval</label><float><property required="no"><keyword>trap=</keyword><value/><default/></property>
	
	
	
      </float></parameter><parameter><label>Zero any value whose magnitude exceeds clipval</label><float><property required="no"><keyword>clip=</keyword><value/><default/></property>
	
	
	
      </float></parameter><parameter><label>Clip any value greater than</label><float><property required="no"><keyword>pclip=</keyword><value/><default/></property></float></parameter><parameter><label>Clip any value less than</label><float><property required="no"><keyword>nclip=</keyword><value/><default/></property></float></parameter><parameter><label>Clip by quantile on absolute values on trace</label><float><property required="no"><keyword>qclip=</keyword><value/><default/></property>
	
	
	
      </float></parameter></parameters></group></parameter>
      
      
      <parameter><label>Data balancing</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0" selection=""><parameter><label>Balance traces by qclip and scale</label><flag><property required="no"><keyword>qbal=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter><parameter><label>Balance traces by dividing by rms value</label><flag><property required="no"><keyword>pbal=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter><parameter><label>Balance traces by subtracting the mean</label><flag><property required="no"><keyword>mbal=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter><parameter><label>Balance traces by subtracting the maximum</label><flag><property required="no"><keyword>maxbal=1</keyword><value>off</value><default>off</default></property></flag></parameter></parameters></template-instance><parameters default-selection="0" selection=""><parameter><label>Balance traces by qclip and scale</label><flag><property required="no"><keyword>qbal=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter><parameter><label>Balance traces by dividing by rms value</label><flag><property required="no"><keyword>pbal=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter><parameter><label>Balance traces by subtracting the mean</label><flag><property required="no"><keyword>mbal=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter><parameter><label>Balance traces by subtracting the maximum</label><flag><property required="no"><keyword>maxbal=1</keyword><value>off</value><default>off</default></property></flag></parameter></parameters></group></parameter>
      
      
      <parameter><label>Multiply data by overall scale factor</label><float><property required="no"><keyword>scale=</keyword><value/><default/></property>
	
	
	
      </float></parameter>
      <parameter><label>Divide data by overall scale factor</label><float><property required="no"><keyword>norm=</keyword><value/><default/></property>
	
	
	
      </float></parameter>
      <parameter><label>Bias data by adding an overall bias value</label><float><property required="no"><keyword>bias=</keyword><value/><default/></property>
	
	
	
      </float></parameter>
      <parameter><label>Selects the parameter choice discussed (Tpow=2, gpow=.5, qclip=.95)</label><flag><property required="no"><keyword>jon=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter>
      <parameter><label>Echoes information</label><flag><property required="no"><keyword>verbose=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter>
      <parameter><label>Directory path prefix for storing temporary files</label><file directory="yes"><property required="no"><keyword>tmpdir=</keyword><value/><default/></property>
	
	
	
      </file></parameter>
    <parameter><label>Apply gain to traces with "Mark selected traces" keyword</label><enum><property required="no"><keyword>mark=</keyword><value/><default>0</default></property><option><label>equal to zero</label><value>0</value></option><option><label>different from zero</label><value>1</value></option></enum></parameter><parameter><label>Sample rate in seconds</label><float><property required="no"><keyword>dt=</keyword><value/><default/></property></float></parameter></parameters>
  </program>
