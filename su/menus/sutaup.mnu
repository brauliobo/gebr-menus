    <parameters default-selection="0" selection="">
      <parameter>
        <label>Computation selection</label>
        <enum>
          <property required="yes">
            <keyword>option=</keyword>
            <value />
            <default>1</default>
          </property>
          <option>
            <label>Forward F-K domain</label>
            <value>1</value>
          </option>
          <option>
            <label>Forward T-X domain</label>
            <value>2</value>
          </option>
          <option>
            <label>Inverse F-K domain</label>
            <value>3</value>
          </option>
          <option>
            <label>Inverse T-X domain</label>
            <value>4</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Time sampling interval (in secs)</label>
        <float>
          <property required="no">
            <keyword>dt=</keyword>
            <value />
            <default />
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of horizontal samples</label>
        <int>
          <property required="no">
            <keyword>nx=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Horizontal sampling interval (in m)</label>
        <float>
          <property required="no">
            <keyword>dx=</keyword>
            <value />
            <default />
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of points for rho filter</label>
        <int>
          <property required="no">
            <keyword>npoints=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Tau-P transform</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0" selection="">
              <parameter>
                <label>Minimum slope for Tau-P transform (s/m)</label>
                <float>
                  <property required="no">
                    <keyword>pmin=</keyword>
                    <value />
                    <default />
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Maximum slope for Tau-P transform (s/m)</label>
                <float>
                  <property required="no">
                    <keyword>pmax=</keyword>
                    <value />
                    <default />
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Number of slopes for Tau-P transform</label>
                <int>
                  <property required="no">
                    <keyword>np=</keyword>
                    <value />
                    <default />
                  </property>
                </int>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0" selection="">
            <parameter>
              <label></label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default></default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label></label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default></default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label></label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default></default>
                </property>
              </reference>
            </parameter>
          </parameters>
        </group>
      </parameter>
      <parameter>
        <label>Minimum frequency of interest</label>
        <float>
          <property required="no">
            <keyword>fmin=</keyword>
            <value />
            <default />
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Directory to store temporary file</label>
        <file directory="yes" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>tmpdir=</keyword>
            <value></value>
            <default />
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Offset on first trace</label>
        <float>
          <property required="no">
            <keyword>xmin=</keyword>
            <value />
            <default />
          </property>
        </float>
      </parameter>
    </parameters>
  </program>
</flow>
