    <parameters default-selection="0">
      <parameter>
        <label>Exponent of complex amplitude</label>
        <float>
          <property required="no">
            <keyword>a=</keyword>
            <value />
            <default>0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Multiplier of complex phase</label>
        <float>
          <property required="no">
            <keyword>b=</keyword>
            <value />
            <default>0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of time samples output</label>
        <int>
          <property required="no">
            <keyword>ntout=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Symmetric output</label>
        <int>
          <property required="no">
            <keyword>sym=</keyword>
            <value />
            <default>0</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
