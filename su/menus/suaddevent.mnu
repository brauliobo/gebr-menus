    <parameters default-selection="0" selection="">
      <parameter><label>Linear event instead of hyperbolic</label><flag><property required="no"><keyword>type=lmo</keyword><value>off</value><default>off</default></property>
        
        
        
      </flag></parameter>
      <parameter><label>Zero-offset intercept time</label><float><property required="no"><keyword>t0=</keyword><value/><default>1.0</default></property>
        
        
        
      </float></parameter>
      <parameter><label>Moveout velocity in m/s</label><float><property required="no"><keyword>vel=</keyword><value/><default>3000.0</default></property>
        
        
        
      </float></parameter>
      <parameter><label>Amplitude</label><float><property required="no"><keyword>amp=</keyword><value/><default>1.0</default></property>
        
        
        
      </float></parameter>
      <parameter><label>Time sample rate</label><float><property required="no"><keyword>dt=</keyword><value/><default/></property>
        
        
        
      </float></parameter>
    </parameters>
  </program>
