    <url>http://www.cwp.mines.edu/cwpcodes/</url>
    <parameters default-selection="0" selection="">
      <parameter>
        <label>Maximum number of midpoints</label>
        <int>
          <property required="yes">
            <keyword>nxmax=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Option</label>
        <enum>
          <property required="no">
            <keyword>option=</keyword>
            <value />
            <default>1</default>
          </property>
          <option>
            <label>Traditional Hale DMO</label>
            <value>1</value>
          </option>
          <option>
            <label>Bleistein's true amplitude DMO</label>
            <value>2</value>
          </option>
          <option>
            <label>Bleistein's cossine times cossine weighted DMO</label>
            <value>3</value>
          </option>
          <option>
            <label>Zhang's DMO</label>
            <value>4</value>
          </option>
          <option>
            <label>Tsvankin's anisotropic DMO</label>
            <value>5</value>
          </option>
          <option>
            <label>Tsvankin's VTI DMO weak anisotropy approximation</label>
            <value>6</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Midpoint sampling interval (meters)</label>
        <float>
          <property required="no">
            <keyword>dx=</keyword>
            <value />
            <default>50</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Velocity (meters/sec)</label>
        <float>
          <property required="no">
            <keyword>v=</keyword>
            <value />
            <default>1500</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Source-receiver half-offset (meters)</label>
        <float>
          <property required="no">
            <keyword>h=</keyword>
            <value />
            <default>200</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of time samples to pad</label>
        <int>
          <property required="no">
            <keyword>ntpad=</keyword>
            <value />
            <default>0</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of midpoints to pad</label>
        <int>
          <property required="no">
            <keyword>nxpad=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Name of file with Velocity NMO</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>file=</keyword>
            <value></value>
            <default />
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Thompsen's epsilon</label>
        <float>
          <property required="no">
            <keyword>e=</keyword>
            <value />
            <default>0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Thompsen's delta</label>
        <float>
          <property required="no">
            <keyword>d=</keyword>
            <value />
            <default>0</default>
          </property>
        </float>
      </parameter>
    </parameters>
  </program>
</flow>
