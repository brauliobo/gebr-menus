                <property required="no">
                  <keyword>nxm=</keyword>
                  <value/>
                  <default>101</default>
                </property>
              </int>
            </parameter>
            <parameter>
              <label>Midpoint sampling interval (km)</label>
              <float>
                <property required="no">
                  <keyword>dxm=</keyword>
                  <value/>
                  <default>0.05</default>
                </property>
