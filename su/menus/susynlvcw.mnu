                  <value/>
                  <default>0.0</default>
                </property>
              </float>
            </parameter>
            
          </parameters></template-instance>
        <parameters default-selection="0" selection="">
            <parameter>
              <label/>
              
            <reference><property required="no"><keyword/><value/><default/></property></reference></parameter><parameter>
              <label/>
              
            <reference><property required="no"><keyword/><value/><default>1</default></property></reference></parameter>
            <parameter>
              <label/>
              
            <reference><property required="no"><keyword/><value/><default>0.05</default></property></reference></parameter>
            <parameter>
              <label/>
              
            <reference><property required="no"><keyword/><value/><default>0.0</default></property></reference></parameter>
            
          </parameters></group>
      </parameter>
