    <parameters default-selection="0">
      <parameter>
        <label>Constant phase shift (a)</label>
        <float>
          <property required="no">
            <keyword>a=</keyword>
            <value/>
            <default>90</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Linear phase shift (b)</label>
        <float>
          <property required="no">
            <keyword>b=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Coefficient (c)</label>
        <float>
          <property required="no">
            <keyword>c=</keyword>
            <value/>
            <default>0.0</default>
          </property>
        </float>
      </parameter>
    </parameters>
