    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of shots</label>
        <int>
          <property required="no">
            <keyword>nshot=</keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of receivers</label>
        <int>
          <property required="no">
            <keyword>nrec=</keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Speed</label>
        <float>
          <property required="no">
            <keyword>c=</keyword>
            <value/>
            <default>5000</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Sampling rate</label>
        <float>
          <property required="no">
            <keyword>dt=</keyword>
            <value/>
            <default>0.004</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of samples</label>
        <int>
          <property required="no">
            <keyword>nt=</keyword>
            <value/>
            <default>256</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>X dimension properties</label>
        <group instanciable="no" expand="no">
          <template-instance><parameters default-selection="0" selection="">
            <parameter>
              <label>Point scatterer location</label>
              <float>
                <property required="no">
                  <keyword>x0=</keyword>
                  <value/>
                  <default>1000</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>First shot location</label>
              <float>
                <property required="no">
                  <keyword>sxmin=</keyword>
                  <value/>
                  <default>0</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>First receiver location</label>
              <float>
                <property required="no">
                  <keyword>gxmin=</keyword>
                  <value/>
                  <default>0</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>X-step in shot location</label>
              <float>
                <property required="no">
                  <keyword>dsx=</keyword>
                  <value/>
                  <default>100</default>
                </property>
              </float>
            </parameter>
