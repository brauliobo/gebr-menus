        <group instanciable="no" expand="no">
          <template-instance><parameters default-selection="0" selection="">
            <parameter>
              <label>Number of interfaces</label>
              <int>
                <property required="no">
                  <keyword>ninf=</keyword>
                  <value/>
                  <default>4</default>
                </property>
              </int>
            </parameter>
            <parameter>
              <label>Dips of interfaces in degrees</label>
              <float>
                <property required="no">
                  <keyword>dip=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Z-intercepts of interfaces</label>
              <float>
                <property required="no">
                  <keyword>zint=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Velocities below surface &amp; interfaces</label>
              <float>
                <property required="no">
                  <keyword>v=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Densities below surface &amp; interfaces</label>
              <float>
                <property required="no" separator=",">
                  <keyword>rho=</keyword>
                  <value/>
                  <default>1</default>
                  <default>1</default>
                  <default>1</default>
                  <default>1</default>
                  <default>1</default>
                </property>
              </float>
            </parameter>
          </parameters></template-instance>
        <parameters default-selection="0" selection="">
            <parameter>
              <label>Number of interfaces</label>
              <int>
                <property required="no">
                  <keyword>ninf=</keyword>
                  <value/>
                  <default>4</default>
                </property>
              </int>
            </parameter>
            <parameter>
              <label>Dips of interfaces in degrees</label>
              <float>
                <property required="no">
                  <keyword>dip=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Z-intercepts of interfaces</label>
              <float>
                <property required="no">
                  <keyword>zint=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Velocities below surface &amp; interfaces</label>
              <float>
                <property required="no">
