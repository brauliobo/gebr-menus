    <url>http://www.gebrproject.com/</url>
    <parameters default-selection="0">
      <parameter>
        <label>Full qualified SU version</label>
        <string>
          <property required="no">
            <keyword>-V</keyword>
            <value/>
            <default>43R1</default>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>SU version prefix</label>
        <string>
          <property required="no">
            <keyword>-p</keyword>
            <value/>
            <default>43</default>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>SU archive</label>
        <string>
          <property required="no">
            <keyword>-a</keyword>
            <value/>
            <default>cwp_su_all_43R1.tgz</default>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>CWP FTP URL</label>
        <string>
          <property required="no">
            <keyword>-d</keyword>
            <value/>
            <default>ftp://ftp.cwp.mines.edu/pub/cwpcodes</default>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>Download path</label>
        <file directory="yes" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>-o</keyword>
            <value/>
            <default>&lt;HOME&gt;/Downloads</default>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Uninstall SU</label>
        <flag>
          <property required="no">
            <keyword>-u</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Keep previously installed SU</label>
        <flag>
          <property required="no">
            <keyword>-U</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
</flow>
