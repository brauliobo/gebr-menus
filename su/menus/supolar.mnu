    <parameters default-selection="0" selection="">
      <parameter>
        <label>Time sampling intervall in seconds</label>
        
      <float><property required="no"><keyword>dt=</keyword><value/><default/></property></float></parameter>
      <parameter>
        <label>Correlation window length in seconds</label>
        <float>
          <property required="no">
            <keyword>wl=</keyword>
            <value/>
            <default>0.1</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Correlation window shape</label>
        <enum>
          <property required="no">
            <keyword>win=</keyword>
            <value/>
            <default>boxcar</default>
          </property>
          <option>
            <label>Boxcar</label>
            <value>boxcar</value>
          </option>
          <option>
            <label>Hanning</label>
            <value>hanning</value>
          </option>
          <option>
            <label>Bartlett</label>
            <value>bartlett</value>
          </option>
          <option>
            <label>Welsh</label>
            <value>welsh</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Base of output file name(s)</label>
        <string>
          <property required="no">
            <keyword>file=</keyword>
            <value/>
            <default>polar</default>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>Rectilinearity</label>
        <enum>
          <property required="no">
            <keyword>rl=</keyword>
            <value/>
            <default>1</default>
          </property>
          <option>
            <label>Rectilinearity evaluating 2 eigenvalues (0 to 1)</label>
            <value>1</value>
          </option>
          <option>
            <label>Rectilinearity evaluating 3 eigenvalues (0 to 1)</label>
            <value>2</value>
          </option>
        <option><label>Rectilinearity evaluating 3 eigenvalues (-1 to 1)</label><value>3</value></option></enum>
      </parameter>
      <parameter>
        <label>Contrast parameter for rectilinearity</label>
        <float>
          <property required="no">
            <keyword>rlq=</keyword>
            <value/>
            <default>1.0</default>
          </property>
        </float>
      </parameter>
      <parameter><label>Create SU Files of:</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0" selection=""><parameter>
        <label>3 components of polarization direction</label>
        
      <flag><property required="no"><keyword>dir=1</keyword><value>off</value><default>on</default></property></flag></parameter><parameter>
        <label>Global polarization parameter</label>
        <flag>
          <property required="no">
            <keyword>tau=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Principal, subprincipal, and transverse ellipticities</label>
        <flag>
          <property required="no">
            <keyword>ellip=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Planarity measure</label>
        <flag>
          <property required="no">
            <keyword>pln=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Flatness or oblateness coefficient</label>
        <flag>
          <property required="no">
            <keyword>f1=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Linearity coefficient</label>
        <flag>
          <property required="no">
            <keyword>l1=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Amplitude parameters</label>
        <flag>
          <property required="no">
            <keyword>amp=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter></parameters></template-instance><parameters default-selection="0" selection=""><parameter>
        <label>3 components of polarization direction</label>
        
      <flag><property required="no"><keyword>dir=1</keyword><value>off</value><default>on</default></property></flag></parameter><parameter>
        <label>Global polarization parameter</label>
        <flag>
          <property required="no">
            <keyword>tau=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Principal, subprincipal, and transverse ellipticities</label>
        <flag>
          <property required="no">
            <keyword>ellip=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Planarity measure</label>
        <flag>
          <property required="no">
            <keyword>pln=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Flatness or oblateness coefficient</label>
        <flag>
          <property required="no">
            <keyword>f1=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Linearity coefficient</label>
        <flag>
          <property required="no">
            <keyword>l1=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter><parameter>
        <label>Amplitude parameters</label>
        <flag>
          <property required="no">
            <keyword>amp=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter></parameters></group></parameter>
      
      
      
      
      
      
      <parameter>
        <label>Incidence angle of principal axis</label>
        
      <enum><property required="no"><keyword>theta=</keyword><value/><default/></property><option><label>from -pi/2 to pi/2</label><value>1</value></option><option><label>from 0 to pi/2</label><value>2</value></option></enum></parameter>
      <parameter>
        <label>Horizontal azimuth of principal axis</label>
        
      <enum><property required="no"><keyword>phi=</keyword><value/><default/></property><option><label>from -pi/2 to pi/2</label><value>1</value></option><option><label>from -pi to pi</label><value>2</value></option><option><label>from 0 to 2 pi</label><value>3</value></option></enum></parameter>
      <parameter>
        <label>Unit of angles</label>
        <enum>
          <property required="no">
            <keyword>angle=</keyword>
            <value/>
            <default>rad</default>
          </property>
          <option>
            <label>Radian</label>
            <value>rad</value>
          </option>
          <option>
            <label>Degree</label>
            <value>deg</value>
          </option>
          <option>
            <label>Grad</label>
            <value>gon</value>
          </option>
        </enum>
      </parameter>
      
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
