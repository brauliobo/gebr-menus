    <parameters default-selection="0">
      
    <parameter>
        <label>Filter</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0">
              
              
            <parameter>
                <label>List of filter values</label>
                <float>
                  <property required="yes" separator=",">
                    <keyword>filter=</keyword>
                    <value/>
                    <default/>
                  </property>
                </float>
              </parameter><parameter>
                <label>File containing SU traces to use as filter</label>
                <file directory="no" filter-name="" filter-pattern="">
                  <property required="yes">
                    <keyword>sufile=</keyword>
                    <value/>
                    <default/>
                  </property>
                </file>
              </parameter></parameters>
          </template-instance>
          <parameters default-selection="1" selection="1">
            
            
          <parameter>
                <label>List of filter values</label>
                
              <reference><property required="no"><keyword/><value/><default/></property></reference></parameter><parameter>
                <label>File containing SU traces to use as filter</label>
                
              <reference><property required="no"><keyword/><value/><default></default></property></reference></parameter></parameters>
        </group>
      </parameter><parameter><label>Traces to use as filter</label><enum><property required="no"><keyword>panel=</keyword><value/><default>0</default></property><option><label>Only the first trace of sufile</label><value>0</value></option><option><label>Trace by trace an entire gather</label><value>1</value></option></enum></parameter><parameter><label>White noise factor for stabilizing results</label><float><property required="no"><keyword>pnoise=</keyword><value/><default>0.001</default></property></float></parameter><parameter><label>Center the output on each trace</label><flag><property required="no"><keyword>sym=1</keyword><value>off</value><default>off</default></property></flag></parameter><parameter><label>Echoes extra information</label><flag><property required="no"><keyword>verbose=1</keyword><value>off</value><default>off</default></property></flag></parameter></parameters>
  </program>
