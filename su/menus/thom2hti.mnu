    <url>http://www.cwp.mines.edu/cwpcodes/</url>
    <parameters default-selection="0">
      <parameter>
        <label>Symm.axis p-wave velocity</label>
        <float>
          <property required="no">
            <keyword>vp=</keyword>
            <value/>
            <default>2</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Symm.axis s-wave velocity</label>
        <float>
          <property required="no">
            <keyword>vs=</keyword>
            <value/>
            <default>1</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Thomsen's generic epsilon</label>
        <float>
          <property required="no">
            <keyword>eps=</keyword>
            <value/>
            <default>0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Thomsen's generic gamma</label>
        <float>
          <property required="no">
            <keyword>gamma=</keyword>
            <value/>
            <default>0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Compute weak approximation</label>
        <int>
          <property required="no">
            <keyword>weak=</keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
    <parameter><label>Output parameter file</label><file directory="no" filter-name="" filter-pattern=""><property required="no"><keyword>outpar=</keyword><value/><default>/dev/stdout</default></property></file></parameter></parameters>
  </program>
