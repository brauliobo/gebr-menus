            <parameter>
              <label>Minimum horizontal coordinate (x)</label>
              <float>
                <property required="no">
                  <keyword>xmin=</keyword>
                  <value/>
                  <default>0.0</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Maximum horizontal coordinate (x)</label>
              <float>
                <property required="no">
                  <keyword>xmax=</keyword>
                  <value/>
                  <default>1.0</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Minimum vertical coordinate (z)</label>
              <float>
                <property required="no">
                  <keyword>zmin=</keyword>
                  <value/>
                  <default>0.0</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Maximum vertical coordinate (z)</label>
              <float>
                <property required="no">
                  <keyword>zmax=</keyword>
                  <value/>
                  <default>1.0</default>
                </property>
              </float>
            </parameter>
          </parameters></template-instance>
        <parameters default-selection="0" selection="">
            <parameter>
              <label>Minimum horizontal coordinate (x)</label>
              <float>
                <property required="no">
                  <keyword>xmin=</keyword>
                  <value/>
                  <default>0.0</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Maximum horizontal coordinate (x)</label>
              <float>
                <property required="no">
                  <keyword>xmax=</keyword>
                  <value/>
                  <default>1.0</default>
                </property>
              </float>
            </parameter>
            <parameter>
              <label>Minimum vertical coordinate (z)</label>
              <float>
                <property required="no">
                  <keyword>zmin=</keyword>
                  <value/>
                  <default>0.0</default>
                </property>
              </float>
            </parameter>
            <parameter>
