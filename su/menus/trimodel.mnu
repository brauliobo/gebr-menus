            <parameter>
              <label>X coordinates of an edge</label>
              
            <float><property required="no" separator=","><keyword>xedge=</keyword><value/><default/></property></float></parameter>
            <parameter>
              <label>Z coordinates of an edge</label>
              
            <float><property required="no" separator=","><keyword>zedge=</keyword><value/><default/></property></float></parameter>
            <parameter>
              <label>Sloth along an edge</label>
              
            <float><property required="no" separator=","><keyword>sedge=</keyword><value/><default/></property></float></parameter>
            
          </parameters></template-instance>
        <parameters default-selection="0" selection="">
            <parameter>
              <label>X coordinates of an edge</label>
              
            <float><property required="no" separator=","><keyword>xedge=</keyword><value/><default/></property></float></parameter>
            <parameter>
              <label>Z coordinates of an edge</label>
              
            <float><property required="no" separator=","><keyword>zedge=</keyword><value/><default/></property></float></parameter>
            <parameter>
              <label>Sloth along an edge</label>
              
            <float><property required="no" separator=","><keyword>sedge=</keyword><value/><default/></property></float></parameter>
            
          </parameters></group>
      </parameter>
      
      <parameter>
              <label>Array of indices used to identify edges</label>
              
            <int><property required="no" separator=","><keyword>kedge=</keyword><value/><default/></property></int></parameter><parameter>
        <label>Specify which interface to shoot rays</label>
        <int>
          <property required="no">
            <keyword>normface=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter><parameter>
        <label>Does generate parameters</label>
        <flag>
          <property required="no">
            <keyword>normray=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Number of locations to shoot rays</label>
        <int>
          <property required="no">
            <keyword>nrays=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter><label>Region filling parameters</label><group instanciable="yes" expand="no"><template-instance><parameters default-selection="0" selection=""><parameter><label>X, Z, x0, z0, s00, dsdx, dsdz to fill a region</label><float><property required="no" separator=","><keyword>sfill=</keyword><value/><default/></property></float></parameter><parameter><label>X, Z, and density to fill a region</label><float><property required="no" separator=","><keyword>densfill=</keyword><value/><default/></property></float></parameter><parameter><label>X, Z, and Q-factor to fill a region</label><float><property required="no" separator=","><keyword>qfill=</keyword><value/><default/></property></float></parameter></parameters></template-instance><parameters default-selection="0" selection=""><parameter><label>X, Z, x0, z0, s00, dsdx, dsdz to fill a region</label><float><property required="no" separator=","><keyword>sfill=</keyword><value/><default/></property></float></parameter><parameter><label>X, Z, and density to fill a region</label><float><property required="no" separator=","><keyword>densfill=</keyword><value/><default/></property></float></parameter><parameter><label>X, Z, and Q-factor to fill a region</label><float><property required="no" separator=","><keyword>qfill=</keyword><value/><default/></property></float></parameter></parameters></group></parameter><parameter>
        <label>Maximum angle (deg) between adjacent edge segments</label>
