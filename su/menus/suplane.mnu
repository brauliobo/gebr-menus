      <parameter><label>Taper planes to zero at the end</label><flag><property required="no"><keyword>taper=1</keyword><value>off</value><default>off</default></property>
	
	
	
      </flag></parameter>
      <parameter><label>Offset</label><float><property required="no"><keyword>offset=</keyword><value/><default>400</default></property>
	
	
	
      </float></parameter>
      <parameter><label>Time sample interval in seconds</label><float><property required="no"><keyword>dt=</keyword><value/><default>0.004</default></property>
	
	
	
      </float></parameter>
