      <parameter><label>Number of planes</label><range min="1" max="3" inc="1" digits=""><property required="no"><keyword>npl=</keyword><value/><default>3</default></property>
	
	
	
      </range></parameter>
      <parameter><label>Number of time samples</label><int min="1"><property required="no"><keyword>nt=</keyword><value/><default>64</default></property>
	
	
	
