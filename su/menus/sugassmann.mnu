  <div class="outer-container">
    <div class="inner-container">
      <div class="header">
        <div class="title">
          <!-- begin tt2 -->
         <span class="flowtitle">SU Gassmann</span>
         <!-- end tt2 -->

          <div class="description">
            <!-- begin des -->
            Model reflectivity change with rock/fluid properties
            <!-- end des -->
