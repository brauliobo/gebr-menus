    <parameters default-selection="0">
      <parameter>
        <label>Number of time samples</label>
        <int>
          <property required="no">
            <keyword>nt=</keyword>
            <value />
            <default>500</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of traces</label>
        <int>
          <property required="no">
            <keyword>ntr=</keyword>
            <value />
            <default>200</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Time sampling interval in seconds</label>
        <float>
          <property required="no">
            <keyword>dt=</keyword>
            <value />
            <default>0.004</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Operation mode</label>
        <enum>
          <property required="no">
            <keyword>mode=</keyword>
            <value />
            <default>0</default>
          </property>
          <option>
            <label>Model isolated gassmann refl coefficient</label>
            <value>0</value>
          </option>
          <option>
            <label>Embed gassmann RC in random RC series</label>
            <value>1</value>
          </option>
          <option>
            <label>R0 parameter sensitivity output</label>
            <value>2</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Parameter sensitivity test range</label>
        <float>
          <property required="no">
            <keyword>p=</keyword>
            <value />
            <default>0.15</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Environment parameters</label>
        <group instanciable="no" expand="no">
          <template-instance>
