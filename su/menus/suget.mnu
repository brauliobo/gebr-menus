    <parameters default-selection="0" selection="">
      <parameter>
        <label>File descriptor for input stream</label>
        <int>
          <property required="no">
            <keyword>fd=</keyword>
            <value/>
            <default>-1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
