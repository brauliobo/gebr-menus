    <parameters default-selection="0" selection="">
      <parameter>
        <label>Operation</label>
        <group instanciable="no" expand="yes">
          <template-instance><parameters default-selection="1" selection="1">
            <parameter>
              <label>Comom operations</label>
              <enum>
                <property required="no">
                  <keyword>power=</keyword>
                  <value/>
                  <default>0</default>
                </property>
                <option>
                  <label>Differentiation</label>
                  <value>-1</value>
                </option>
                <option>
                  <label>Semi-differentiation</label>
                  <value>-0.5</value>
                </option>
                <option>
                  <label>Phase shift only</label>
                  <value>0</value>
                </option>
                <option>
                  <label>Semi-integration</label>
                  <value>0.5</value>
                </option>
                <option>
                  <label>Integration</label>
                  <value>1</value>
                </option>
              </enum>
            </parameter>
            <parameter>
              <label>Exponent of (-i omega) term</label>
              <float>
                <property required="no">
                  <keyword>power=</keyword>
                  <value/>
                  <default/>
                </property>
              </float>
            </parameter>
          </parameters></template-instance>
        <parameters default-selection="1" selection="1">
            <parameter>
              <label></label>
              
            <reference><property required="no"><keyword/><value></value><default>0</default></property></reference></parameter>
            <parameter>
              <label></label>
              
            <reference><property required="no"><keyword/><value></value><default></default></property></reference></parameter>
          </parameters></group>
      </parameter>
      <parameter>
        <label>Use positive sign in (i omega) term</label>
        <flag>
          <property required="no">
            <keyword>sign=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Phase shift value</label>
        <float>
          <property required="no">
            <keyword>phasefac=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
</flow>
