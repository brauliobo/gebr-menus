    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of samples in the fast (first) dimension</label>
        <int>
          <property required="yes">
            <keyword>n1=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of samples in the slow (second) dimension</label>
        <int>
          <property required="yes">
            <keyword>n2=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of stages in the fast dimension</label>
        <int>
          <property required="no">
            <keyword>nstage1=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of stages in the slow dimension</label>
        <int>
          <property required="no">
            <keyword>nstage2=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of filters</label>
        <int>
          <property required="no">
            <keyword>nfilter=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Acceptable error</label>
        <float>
          <property required="no">
            <keyword>error=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
    </parameters>
  </program>
