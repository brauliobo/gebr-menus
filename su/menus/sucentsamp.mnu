    <parameters default-selection="0">
      
      <parameter>
        <label>Sampling interval</label>
        <float>
          <property required="no">
            <keyword>dt=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
    <parameter><label>Minimum samples in lobe for inclusion</label><int><property required="no"><keyword>nvals_min=</keyword><value/><default>1</default></property></int></parameter><parameter>
        <label>Stop advisory messages</label>
        <flag>
          <property required="no">
            <keyword>verbose=0</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter></parameters>
