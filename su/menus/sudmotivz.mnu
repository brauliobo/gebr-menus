    <parameters default-selection="0" selection="">
      <parameter>
        <label>Minimum CDP for wich to aply DMO</label>
        <int>
          <property required="yes">
            <keyword>cdpmin=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Maximum CDP for wich to aply DMO</label>
        <int>
          <property required="yes">
            <keyword>cdpmax=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Distance between adjacend CDP bins (meters)</label>
        <float min="0">
          <property required="yes">
            <keyword>dxcdp=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of offsets to mix</label>
        <int>
          <property required="yes">
            <keyword>noffmix=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Binary file containing NMO interval velocities (m/s)</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>vnfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Binary file containing interval velocities (m/s)</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>vfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Binary file containing eta interval values (m/s)</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>etafile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Times corresponding to interval velocities (s)</label>
        <float>
          <property required="no">
            <keyword>tdmo=</keyword>
            <value/>
            <default>0.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Interval velocities corresponding to times (m/s)</label>
        <float>
          <property required="no">
            <keyword>vdmo=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>NMO interval velocities corresponding to times (m/s)</label>
        <float>
          <property required="no">
            <keyword>vndmo=</keyword>
            <value/>
            <default>1500.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Eta interval values corresponding to times (m/s)</label>
        <float>
          <property required="no">
            <keyword>etadmo=</keyword>
            <value/>
            <default>1500.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Maximum frequency in input traces (Hz)</label>
        <float>
          <property required="no">
            <keyword>fmax=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Stretch mute used for NMO correction</label>
        <float>
          <property required="no">
            <keyword>smute=</keyword>
            <value/>
            <default>1.5</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Twist this knob for speed (and aliasing)</label>
        <float>
          <property required="no">
            <keyword>speed=</keyword>
            <value/>
            <default>1.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
