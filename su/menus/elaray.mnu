    <parameters default-selection="0">
      <parameter>
        <label>X coordinate of source</label>
        <float>
          <property required="no">
            <keyword>xs=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Z coordinate of source</label>
        <float>
          <property required="no">
            <keyword>zs=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Takeoff angle properties</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0" selection="">
              
              
              
            <parameter>
                <label>Number of takeoff angles</label>
                <int>
                  <property required="no">
                    <keyword>nangle=</keyword>
                    <value/>
                    <default>101</default>
                  </property>
                </int>
              </parameter><parameter>
                <label>First takeoff angle (in degrees)</label>
                <float>
                  <property required="no">
                    <keyword>fangle=</keyword>
                    <value/>
                    <default>-45</default>
                  </property>
                </float>
              </parameter><parameter>
                <label>Last takeoff angle (in degrees)</label>
                <float>
                  <property required="no">
                    <keyword>langle=</keyword>
                    <value/>
                    <default>45</default>
                  </property>
                </float>
              </parameter></parameters>
          </template-instance>
          <parameters default-selection="0" selection="">
            
            
            
          <parameter>
                <label></label>
                
              <reference><property required="no"><keyword/><value/><default>101</default></property></reference></parameter><parameter>
                <label></label>
                
              <reference><property required="no"><keyword/><value/><default>-45</default></property></reference></parameter><parameter>
                <label></label>
                
              <reference><property required="no"><keyword/><value/><default>45</default></property></reference></parameter></parameters>
        </group>
      </parameter>
      <parameter>
        <label>File of ray x,z coordinates of ray-edge intersections</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>rayfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter><parameter>
        <label>Number of (x,z) in file of ray-edge intersections coordinates</label>
        <int min="0">
          <property required="no">
            <keyword>nxz=</keyword>
            <value/>
            <default>101</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>File of ray x,z coordinates uniformly sampled in time</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>wavefile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter><parameter>
        <label>Number of (x,z) in file of uniformly sampled in time coordinates</label>
        <int min="0">
          <property required="no">
            <keyword>nt=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter><parameter>
        <label>Mode of shoot</label>
        <enum>
          <property required="no">
            <keyword>mode=</keyword>
            <value/>
            <default>0</default>
          </property>
          <option>
            <label>Shoot P-rays</label>
            <value>0</value>
          </option>
          <option>
            <label>Shoot SV-rays</label>
            <value>1</value>
          </option>
          <option>
            <label>Shoot SH-rays</label>
            <value>2</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Rays to display</label>
        <enum>
          <property required="no">
            <keyword>prim=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>Only single-reflected rays</label>
            <value>1</value>
          </option>
          <option>
            <label>Only direct hits</label>
            <value>0</value>
          </option>
        </enum>
      </parameter>
      <parameter><label>Index of reflectors</label><group instanciable="yes" expand="yes"><template-instance><parameters default-selection="0"><parameter>
        <label>Index of reflector</label>
        <int>
          <property required="no" separator=",">
            <keyword>refseq=</keyword>
            <value/>
            <default>1</default>
            <default>0</default>
            <default>0</default>
          </property>
        </int>
      </parameter></parameters></template-instance><parameters default-selection="0"><parameter>
        <label>Index of reflector</label>
        
      <reference><property required="no"><keyword/><value/><default>1</default><default>0</default><default>0</default></property></reference></parameter></parameters></group></parameter><parameter>
        <label>Display and store only rays incident at interface with this index value</label>
        <int>
          <property required="no">
            <keyword>krecord=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter><parameter><label>Force impact strenght</label><float><property required="no"><keyword>f0=</keyword><value/><default>1.0</default></property></float></parameter><parameter><label>Force dip with respect to vertical</label><float><property required="no"><keyword>fdip=</keyword><value/><default>0.0</default></property></float></parameter><parameter><label>Force azimuth with respect to positive x-axis</label><float><property required="no"><keyword>fazi=</keyword><value/><default>0.0</default></property></float></parameter><parameter><label>Include reflec/transm coeff (currently only for P)</label><flag><property required="no"><keyword>reftrans=1</keyword><value>off</value><default>off</default></property></flag></parameter>
      
      
      <parameter><label>Traveltime associated with wavefront</label><float><property required="no"><keyword>tw=</keyword><value/><default/></property></float></parameter><parameter>
        <label>ASCII-file to store useful information</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>infofile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      
      
    <parameter><label>File containing parameters for the plotting software</label><file directory="no" filter-name="" filter-pattern=""><property required="no"><keyword>outparfile=</keyword><value/><default/></property></file></parameter></parameters>
  </program>
</flow>
