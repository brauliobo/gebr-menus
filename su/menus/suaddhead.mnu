    <url>http://www.cwp.mines.edu/cwpcodes/</url><parameters default-selection="0" selection="">
      <parameter><label>Number of samples per trace</label><int min="1"><property required="yes"><keyword>ns=</keyword><value/><default/></property>
	
	
	
      </int></parameter>
      <parameter><label>Binary file format</label><enum><property required="no"><keyword>ftn=</keyword><value/><default>0</default></property><option><label>Unformated C data</label><value>0</value></option><option><label>Unformated Fortran data</label><value>1</value></option></enum></parameter>
      <parameter><label>Trace sorting code</label><enum><property required="no"><keyword>tsort=</keyword><value/><default>3</default></property><option><label>As recorded (no sorting)</label><value>1</value></option><option><label>CDP ensemble</label><value>2</value></option><option><label>Single fold continuous profile</label><value>3</value></option><option><label>Horizontally stacked</label><value>4</value></option></enum></parameter>
      <parameter><label>Number of data traces per record</label><int><property required="no"><keyword>ntrpr=</keyword><value/><default>1</default></property>
	
	
	
      </int></parameter>
