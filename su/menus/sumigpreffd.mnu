    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of total horizontal output samples</label>
        <int>
          <property required="yes">
            <keyword>nxo=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of shot gathers to be migrated</label>
        <int>
          <property required="yes">
            <keyword>nxshot=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of depth samples</label>
        <int>
          <property required="yes">
            <keyword>nz=</keyword>
            <value />
            <default />
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Time sampling interval</label>
        <float>
          <property required="no">
            <keyword>dt=</keyword>
            <value />
            <default />
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Horizontal sampling interval</label>
        <float>
          <property required="yes">
            <keyword>dx=</keyword>
            <value />
            <default />
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Depth sampling interval</label>
        <float>
          <property required="yes">
            <keyword>dz=</keyword>
            <value />
            <default />
          </property>
        </float>
      </parameter>
      <parameter>
        <label>File with velocity profile</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="yes">
            <keyword>vfile=</keyword>
            <value></value>
            <default />
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Peak frequency</label>
        <float>
          <property required="no">
            <keyword>fmax=</keyword>
            <value />
            <default>25</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Frequencies to build a Hamming window</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0" selection="">
              <parameter>
                <label>Frequency 1</label>
                <float>
                  <property required="no">
                    <keyword>f1=</keyword>
                    <value />
                    <default>5</default>
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Frequency 2</label>
                <float>
                  <property required="no">
                    <keyword>f2=</keyword>
                    <value />
                    <default>10</default>
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Frequency 3</label>
                <float>
                  <property required="no">
                    <keyword>f3=</keyword>
                    <value />
                    <default>40</default>
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Frequency 4</label>
                <float>
                  <property required="no">
                    <keyword>f4=</keyword>
                    <value />
                    <default>50</default>
                  </property>
                </float>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0" selection="">
            <parameter>
              <label></label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default>5</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label></label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default>10</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label></label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default>40</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label></label>
              <reference>
                <property required="no">
                  <keyword />
                  <value></value>
                  <default>50</default>
                </property>
              </reference>
            </parameter>
          </parameters>
        </group>
      </parameter>
      <parameter>
        <label>Number of zero traces padded on left</label>
        <int>
          <property required="no">
            <keyword>lpad=</keyword>
            <value />
            <default>9999</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of zero traces padded on right</label>
        <int>
          <property required="no">
            <keyword>rpad=</keyword>
            <value />
            <default>9999</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Echoes information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
