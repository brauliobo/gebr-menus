    <parameters default-selection="0" selection="">
      <parameter>
        <label>Source layer number</label>
        <int min="1">
          <property required="no">
            <keyword>l=</keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Receiver layer number</label>
        <int min="1">
          <property required="no">
            <keyword>k=</keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of output time-samples</label>
        <int min="" max="">
          <property required="no">
            <keyword>tmax=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Type of seismogram</label>
        <enum>
          <property required="no">
            <keyword>pV=</keyword>
            <value/>
            <default>1</default>
          </property>
          <option>
            <label>Vector field seismogram</label>
            <value>1</value>
          </option>
          <option>
            <label>Pressure seismogram</label>
            <value>-1</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Echoes extra information</label>
        <flag>
          <property required="no">
            <keyword>verbose=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    </parameters>
  </program>
