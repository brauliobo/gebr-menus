    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of samples in the fast (first) dimension</label>
        <int>
          <property required="yes">
            <keyword>n1=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of samples in the slow (second) dimension</label>
        <int>
          <property required="yes">
            <keyword>n2=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      
      <parameter><label>Filters properties on fast dimension</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter>
        <label>Number of filters</label>
        <int>
          <property required="no">
            <keyword>nfilter1=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter><parameter>
        <label>Filter stages</label>
        <int>
          <property required="no">
            <keyword>nstage1=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter></parameters></template-instance><parameters default-selection="0"><parameter>
        <label>Filter stages</label>
        
      <reference><property required="no"><keyword/><value/><default></default></property></reference></parameter><parameter>
        <label>Filter stages in fast dimension</label>
        
      <reference><property required="no"><keyword/><value/><default></default></property></reference></parameter></parameters></group></parameter><parameter><label>Filters properties on slow dimension</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter>
        <label>Number of filters</label>
        <int>
          <property required="no">
            <keyword>nfilter2=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter><parameter>
        <label>Filter stages</label>
        <int>
          <property required="no">
            <keyword>nstage2=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter></parameters></template-instance><parameters default-selection="0"><parameter>
        <label>Filter stages</label>
        
      <reference><property required="no"><keyword/><value/><default></default></property></reference></parameter><parameter>
        <label>Filter stages in slow dimension</label>
        
      <reference><property required="no"><keyword/><value/><default></default></property></reference></parameter></parameters></group></parameter>
      
      
      <parameter>
        <label>Acceptable error</label>
        <float>
          <property required="no">
            <keyword>error=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
    </parameters>
