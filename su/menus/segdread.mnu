    <parameters default-selection="0">
      <parameter>
        <label>Input tape device</label>
        <string>
          <property required="yes">
            <keyword>tape=</keyword>
            <value/>
            <default/>
          </property>
        </string>
      </parameter>
      <parameter>
        <label>IO option</label>
        <enum>
          <property required="no">
            <keyword>use_stdio=</keyword>
            <value/>
            <default>0</default>
          </property>
          <option>
            <label>Record devices (9-track reel tape drive)</label>
            <value>0</value>
          </option>
          <option>
            <label>Pipe, disk and fixed-block 8mm drives</label>
            <value>1</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Information mode</label>
        <enum>
          <property required="no">
            <keyword>verbose=</keyword>
            <value/>
            <default>0</default>
          </property>
          <option>
            <label>Quiet mode</label>
            <value>0</value>
          </option>
          <option>
            <label>Echo every 'vblock' traces</label>
            <value>1</value>
          </option>
          <option>
            <label>Echo information about blocks</label>
            <value>2</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Echo every 'vblock' traces under Information mode</label>
        <int>
          <property required="no">
            <keyword>vblock=</keyword>
            <value/>
            <default>50</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>First shot to read</label>
        <int>
          <property required="no">
            <keyword>ptmin=</keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Last shot to read</label>
        <int>
          <property required="no">
            <keyword>ptmax=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Apply gain</label>
        <flag>
          <property required="no">
            <keyword>gain=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Recovery auxiliary traces</label>
        <flag>
          <property required="no">
            <keyword>aux=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Maximum consecutive tape IO errors</label>
        <int>
          <property required="no">
            <keyword>errmax=</keyword>
            <value/>
            <default>0</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of data samples</label>
        <int>
          <property required="no">
            <keyword>ns=</keyword>
            <value/>
            <default>0</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Pivot year</label>
        <int>
          <property required="no">
            <keyword>pivot_year=</keyword>
            <value/>
            <default>30</default>
          </property>
        </int>
      </parameter>
    </parameters>
  </program>
</flow>
