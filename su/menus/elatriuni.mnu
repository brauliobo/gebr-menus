    <parameters default-selection="0">
      <parameter>
        <label>Number of x samples</label>
        <int>
          <property required="yes">
            <keyword>nx=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of z samples</label>
        <int>
          <property required="yes">
            <keyword>nz=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>X sampling interval</label>
        <float>
          <property required="no">
            <keyword>dx=</keyword>
            <value/>
            <default>1.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Z sampling interval</label>
        <float>
          <property required="no">
            <keyword>dz=</keyword>
            <value/>
            <default>1.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>First x sampled</label>
        <float>
          <property required="no">
            <keyword>fx=</keyword>
            <value/>
            <default>0.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>First z sampled</label>
        <float>
          <property required="no">
            <keyword>fz=</keyword>
            <value/>
            <default>0.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
