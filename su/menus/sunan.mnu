    <parameters default-selection="0" selection="">
      <parameter>
        <label>Suppress echo locations of NaNs or Infs</label>
        <flag>
          <property required="no">
            <keyword>verbose=0</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    <parameter><label>NaNs and Inf replacement value</label><float><property required="no"><keyword>value=</keyword><value/><default>0.0</default></property></float></parameter><parameter><label>Replace NaNs and Infs by interpolating neighboring finite values</label><flag><property required="no"><keyword>interp=1</keyword><value>off</value><default>off</default></property></flag></parameter></parameters>
