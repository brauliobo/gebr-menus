    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of input pairs</label>
        <int>
          <property required="yes">
            <keyword>npairs=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Value to scale spikes by</label>
        <float>
          <property required="no">
            <keyword>scale=</keyword>
            <value/>
            <default>1.0</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Dimension of 1st (fast) dimension of output array</label>
        <int>
          <property required="no">
            <keyword>nx1=</keyword>
            <value/>
            <default>100</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Dimension of 2nd (slow) dimension of output array</label>
        <int>
          <property required="no">
            <keyword>nx2=</keyword>
            <value/>
            <default>100</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Zero padding in 1st dimension</label>
        <int>
          <property required="no">
            <keyword>x1pad=</keyword>
            <value/>
            <default>2</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Zero padding in 2nd dimension</label>
        <int>
          <property required="no">
            <keyword>x2pad=</keyword>
            <value/>
            <default>2</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Format of pairs</label>
        <enum>
          <property required="no">
            <keyword>yx=</keyword>
            <value/>
            <default>0</default>
          </property>
          <option>
            <label>Assume (x,y) pairs</label>
            <value>0</value>
          </option>
          <option>
            <label>Assume (y,x) pairs</label>
            <value>1</value>
          </option>
        </enum>
      </parameter>
    </parameters>
  </program>
</flow>
