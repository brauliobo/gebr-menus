    <parameter><label>Operation</label><enum><property required="no"><keyword>op=</keyword><value/><default>abs</default></property><option><label>Absolute Value</label><value>abs</value></option><option><label>Remove average value</label><value>avg</value></option><option><label>Signed square root</label><value>ssqrt</value></option><option><label>Square</label><value>sqr</value></option><option><label>Signed square</label><value>ssqr</value></option><option><label>Signum function</label><value>sgn</value></option><option><label>Exponentiate</label><value>exp</value></option><option><label>Signed natural log</label><value>slog</value></option><option><label>Signed log base 2</label><value>slog2</value></option><option><label>Signed common log</label><value>slog10</value></option><option><label>Cosine</label><value>cos</value></option><option><label>Sine</label><value>sin</value></option><option><label>Tangent</label><value>tan</value></option><option><label>Hyperbolic cosine</label><value>cosh</value></option><option><label>Hyperbolic sine</label><value>sinh</value></option><option><label>Hyperbolic tangent</label><value>tanh</value></option><option><label>Divide trace by Max. Value</label><value>norm</value></option><option><label>20 * Signed common log (data)</label><value>db</value></option><option><label>Negate Value</label><value>neg</value></option><option><label>Pass only positive value</label><value>posonly</value></option><option><label>Pass only negative values</label><value>negonly</value></option><option><label>Running sum trace integration</label><value>sum</value></option><option><label>Running diff trace differentiation</label><value>diff</value></option><option><label>(v[i+1] - v[i]) / ((v[i+1] + v[i])</label><value>refl</value></option><option><label>Modulo 2 pi</label><value>mod2pi</value></option><option><label>Inverse</label><value>inv</value></option><option><label>RMS amplitude</label><value>rmsamp</value></option><option><label>Sonic to velocity (ft/s) conversion</label><value>s2v</value></option><option><label>Sonic to velocity (m/s) conversion</label><value>s2vm</value></option><option><label>Density (g/cc) to metric (kg/m³) conversion</label><value>d2m</value></option><option><label>2nd order vertical derivative</label><value>drv2</value></option><option><label>4th power vertical derivative</label><value>drv4</value></option><option><label>Top-down integration</label><value>integ</value></option><option><label>Local extrema to spikes</label><value>spike</value></option><option><label>Spike and fill to next spike</label><value>saf</value></option><option><label>Local Dominant frequency</label><value>freq</value></option><option><label>Preserve least non-zero amps</label><value>lnza</value></option><option><label>Arithmetic mean</label><value>mean</value></option><option><label>Standard deviation</label><value>std</value></option><option><label>Variance</label><value>var</value></option><option><label>No operation</label><value>nop</value></option></enum></parameter><parameter><label>Number of time samples in window</label><int><property required="no"><keyword>nw=</keyword><value/><default>21</default></property></int></parameter></parameters>
