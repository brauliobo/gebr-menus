    <title>SU Unary Operations</title>
    <binary>suop</binary>
    <description>Do unary arithmetic operation on segys</description>
    <help>
      <![CDATA[<html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    
    <link rel="stylesheet" type="text/css" href="gebr.css" />

    <!-- begin ttl -->
  <title>SU Unary Operations</title>
  <!-- end ttl -->
  </head><body>
    <div class="outer-container">
      <div class="inner-container">
        <div class="header">
          <div class="title">
            <!-- begin tt2 -->
         <span class="flowtitle">SU Unary Operations</span>
         <!-- end tt2 -->
            <div class="description">
           <!-- begin des -->
            Do unary arithmetic operation on segys
            <!-- end des -->
