    <url>http://www.cwp.mines.edu/cwpcodes/</url>
    <parameters default-selection="0" selection="">
      <parameter>
        <label>Show only caustic rays</label>
        <flag>
          <property required="no">
            <keyword>caustic=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Show rays which does not reach surface</label>
        <flag>
          <property required="no">
            <keyword>nonsurface=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Shoot rays from surface</label>
        <flag>
          <property required="no">
            <keyword>surface=1</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Number of locations to shoot rays</label>
        <int>
          <property required="no">
            <keyword>nrays=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Increment of ray angle for one location</label>
        <float>
          <property required="no">
            <keyword>dangle=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of rays shot from one location</label>
        <int>
          <property required="no">
            <keyword>nangle=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Shift first taking off angle</label>
        <float>
          <property required="no">
            <keyword>ashift=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>X coordinate of shooting location</label>
        <float>
          <property required="no">
            <keyword>xs1=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Z coordinate of shooting location</label>
        <float>
          <property required="no">
            <keyword>zs1=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Number of takeoff angles</label>
        <int>
          <property required="no">
            <keyword>nangles=</keyword>
            <value/>
            <default>101</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>First takeoff angle (in degrees)</label>
        <float>
          <property required="no">
            <keyword>fangle=</keyword>
            <value/>
            <default>-45</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>File containing (x,z) ray coordinates</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>rayfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Number of (x,z) pairs in file of ray</label>
        <int>
          <property required="no">
            <keyword>nxz=</keyword>
            <value/>
            <default>101</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>File containing (x,z) wave coordinates</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>wavefile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Number of (x,z) pairs in file of wave</label>
        <int>
          <property required="no">
            <keyword>nt=</keyword>
            <value/>
            <default>101</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>ASCII file to store useful information</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>infofile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>File to plot the fresnel volumes</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>fresnelfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Output parameter file</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>outparfile=</keyword>
            <value/>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Index of rays that are displayed and stored</label>
        <int>
          <property required="no">
            <keyword>krecord=</keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Plot option</label>
        <enum>
          <property required="no">
            <keyword>prim=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>Only single-reflected rays are plotted</label>
            <value>1</value>
          </option>
          <option>
            <label>Only direct hits are plotted</label>
            <value>0</value>
          </option>
        </enum>
      </parameter>
      <parameter>
        <label>Fresnel volume frequency</label>
        <float>
          <property required="no">
            <keyword>ffreq=</keyword>
            <value/>
            <default>-1</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Reflector properties</label>
        <enum>
          <property required="no" separator=",">
            <keyword>refseq=</keyword>
            <value/>
            <default/>
          </property>
          <option>
            <label>Reflection</label>
            <value>1</value>
          </option>
          <option>
            <label>Transmission</label>
            <value>0</value>
          </option>
          <option>
            <label>Ray stops</label>
            <value>-1</value>
          </option>
        </enum>
      </parameter>
    </parameters>
  </program>
</flow>
