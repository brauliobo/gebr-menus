    <parameters default-selection="0">
      <parameter>
        <label>Number of depth samples in Backus average window</label>
        <int>
          <property required="no">
            <keyword>navg=</keyword>
            <value/>
            <default>101</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Parameters for include intrinsic anisotropy of shale layers</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0">
              
              
              
              
              
              
              
            <parameter><label>Input is vp__vs__rho__gr</label><flag><property required="no"><keyword>gr=1</keyword><value>off</value><default>off</default></property></flag></parameter><parameter>
                <label>Pure shale gamma ray value (API units)</label>
                <float>
                  <property required="no">
                    <keyword>grs=</keyword>
                    <value/>
                    <default>100</default>
                  </property>
                </float>
              </parameter><parameter>
                <label>0% shale gamma ray value (API units)</label>
                <float>
                  <property required="no">
                    <keyword>grc=</keyword>
                    <value/>
                    <default>10</default>
                  </property>
                </float>
              </parameter><parameter>
                <label>Include shale anisotropy parameters for</label>
                <enum>
                  <property required="no">
                    <keyword>smode=</keyword>
                    <value/>
                    <default>1</default>
                  </property>
                  <option>
                    <label>Shale volume</label>
                    <value>1</value>
                  </option>
                  <option>
                    <label>Pure shale only</label>
                    <value>0</value>
                  </option>
                </enum>
              </parameter><parameter>
                <label>Shale epsilon (Thomsen parameter)</label>
                <float>
                  <property required="no">
                    <keyword>se=</keyword>
                    <value/>
                    <default>0.209</default>
                  </property>
                </float>
              </parameter><parameter>
                <label>Shale delta (Thomsen parameter)</label>
                <float>
                  <property required="no">
                    <keyword>sd=</keyword>
                    <value/>
                    <default>0.033</default>
                  </property>
                </float>
              </parameter><parameter>
                <label>Shale gamma (Thomsen parameter)</label>
                <float>
                  <property required="no">
                    <keyword>sg=</keyword>
                    <value/>
                    <default>0.203</default>
                  </property>
                </float>
              </parameter></parameters>
          </template-instance>
          <parameters default-selection="0">
            
            
            
            
            
            
            
          <parameter><label>Input is vp__vs__rho__gr</label><reference><property required="no"><keyword/><value/><default>off</default></property></reference></parameter><parameter>
                <label>Pure shale gamma ray value (API units)</label>
                
              <reference><property required="no"><keyword/><value/><default>100</default></property></reference></parameter><parameter>
                <label>0% shale gamma ray value (API units)</label>
                
              <reference><property required="no"><keyword/><value/><default>10</default></property></reference></parameter><parameter>
                <label>Include shale anisotropy parameters for</label>
                
              <reference><property required="no"><keyword/><value/><default>1</default></property></reference></parameter><parameter>
                <label>Shale epsilon (Thomsen parameter)</label>
                
              <reference><property required="no"><keyword/><value/><default>0.209</default></property></reference></parameter><parameter>
                <label>Shale delta (Thomsen parameter)</label>
                
              <reference><property required="no"><keyword/><value/><default>0.033</default></property></reference></parameter><parameter>
                <label>Shale gamma (Thomsen parameter)</label>
                
              <reference><property required="no"><keyword/><value/><default>0.203</default></property></reference></parameter></parameters>
        </group>
      </parameter>
    </parameters>
  </program>
