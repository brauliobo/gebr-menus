    <parameters default-selection="0">
      <parameter>
        <label>HTI Parameters</label>
        <group instanciable="no" expand="no">
          <template-instance>
            <parameters default-selection="0">
              <parameter>
                <label>Isotropy-plane p-wave velocity (alpha)</label>
                <float>
                  <property required="no">
                    <keyword>alpha=</keyword>
                    <value/>
                    <default>2.0</default>
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Fast isotropy-plan s-wave velocity (beta)</label>
                <float>
                  <property required="no">
                    <keyword>beta=</keyword>
                    <value/>
                    <default>1.0</default>
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Parameter e(V)</label>
                <float>
                  <property required="no">
                    <keyword>ev=</keyword>
                    <value/>
                    <default>0.0</default>
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Parameter d(V)</label>
                <float>
                  <property required="no">
                    <keyword>dv=</keyword>
                    <value/>
                    <default>0.0</default>
                  </property>
                </float>
              </parameter>
              <parameter>
                <label>Shear-wave splitting parameter (gamma)</label>
                <float>
                  <property required="no">
                    <keyword>gamma=</keyword>
                    <value/>
                    <default>0.0</default>
                  </property>
                </float>
              </parameter>
            </parameters>
          </template-instance>
          <parameters default-selection="0">
            <parameter>
              <label>Isotropy-plane p-wave velocity (alpha)</label>
              <reference>
                <property required="no">
                  <keyword/>
                  <value/>
                  <default>2.0</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label>Fast isotropy-plan s-wave velocity (beta)</label>
              <reference>
                <property required="no">
                  <keyword/>
                  <value/>
                  <default>1.0</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label>Parameter e(V)</label>
              <reference>
                <property required="no">
                  <keyword/>
                  <value/>
                  <default>0.0</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label>Parameter d(V)</label>
              <reference>
                <property required="no">
                  <keyword/>
                  <value/>
                  <default>0.0</default>
                </property>
              </reference>
            </parameter>
            <parameter>
              <label>Shear-wave splitting parameter (gamma)</label>
              <reference>
                <property required="no">
                  <keyword/>
                  <value/>
                  <default>0.0</default>
                </property>
              </reference>
            </parameter>
          </parameters>
        </group>
      </parameter>
      <parameter>
        <label>Density</label>
        <float>
          <property required="no">
            <keyword>rho=</keyword>
            <value/>
            <default>1.0</default>
          </property>
        </float>
      </parameter>
      
      
      <parameter>
        <label>Sign of c13+c55</label>
        <enum>
          <property required="no">
            <keyword>sign=</keyword>
            <value/>
            <default>1</default>
          </property>
          <option>
            <label>Positive</label>
            <value>1</value>
          </option>
          <option>
            <label>Negative</label>
            <value>-1</value>
          </option>
        </enum>
      </parameter>
    <parameter>
        <label>Output parameter file</label>
        <file directory="no" filter-name="" filter-pattern="">
          <property required="no">
            <keyword>outpar=</keyword>
            <value/>
            <default>/dev/stdout</default>
          </property>
        </file>
      </parameter></parameters>
  </program>
